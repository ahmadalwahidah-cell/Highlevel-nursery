{
  "name": "highlevel-nursery",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:sandbox": "wrangler pages dev dist --d1=webapp-production --local --ip 0.0.0.0 --port 3001",
    "build": "vite build",
    "preview": "wrangler pages dev",
    "deploy": "npm run build && wrangler pages deploy dist --project-name highlevel-nursery",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "backup": "bash scripts/backup-database.sh",
    "backup:with-token": "export CLOUDFLARE_API_TOKEN=$npm_config_token && bash scripts/backup-database.sh",
    "backup:auto": "bash scripts/backup-with-email.sh",
    "backup:to-excel": "python3 scripts/convert-backup-to-excel.py",
    "backup:setup": "bash scripts/setup-auto-backup.sh",
    "restore:list": "ls -lht backups/ | head -20",
    "restore:view": "cat WHERE_TO_FIND_BACKUPS.md",
    "restore:report": "cat backups/latest_backup_report.txt"
  },
  "dependencies": {
    "hono": "^4.10.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251205.0",
    "@hono/vite-build": "^1.7.0",
    "@hono/vite-cloudflare-pages": "^0.4.2",
    "@hono/vite-dev-server": "^0.23.0",
    "terser": "^5.44.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "wrangler": "^4.53.0"
  }
}
